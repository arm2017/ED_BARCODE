#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../html-boilerplate-pdf/bin/html-boilerplate-pdf" "$@"
  ret=$?
else 
  node  "$basedir/../html-boilerplate-pdf/bin/html-boilerplate-pdf" "$@"
  ret=$?
fi
exit $ret
